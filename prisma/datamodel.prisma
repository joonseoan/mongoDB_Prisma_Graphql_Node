type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!

}

type Track {
  id: ID! @id
  user: User! @relation(link: INLINE) 
  name: String! @default(value: "")
  locations: [Point!]! @relation(link: INLINE) 
}

type Point @embedded {
 timestamp: DateTime!
 coords: Coords!
}

type Coords @embedded {
  latitude: Float!
  longitude: Float!
  altitude: Float!
  accuracy: Float!
  heading: Float!
  speed: Float!
}

